{
  "singleQuote": true,
  "printWidth": 120,
  "proseWrap": "always",
  "tabWidth": 4,
  "useTabs": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "semi": true,
  "endOfLine": "auto",
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@softonus/prettier-plugin-whitespace-remover"
  ],
  "tailwindPreserveWhitespace": true
}
